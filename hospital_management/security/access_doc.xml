<odoo>
    <data noupdate="1">
        <!-- Allow doctors to write on wb.room model -->
        <record id="access_doctor_room_write" model="ir.model.access">
            <field name="name">doctor room write access</field>
            <field name="model_id" ref="model_wb_room"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="False"/>
            <field name="perm_unlink" eval="False"/>
        </record>
    </data>
</odoo>
